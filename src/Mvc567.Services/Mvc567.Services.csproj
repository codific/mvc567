  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="8.1.1" />
    <PackageReference Include="DinkToPdf" Version="1.0.8" />
    <PackageReference Include="Magick.NET-Q8-AnyCPU" Version="7.14.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.2.0" />
    <PackageReference Include="NPOI" Version="2.4.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Mvc567.Common\Mvc567.Common.csproj" />
    <ProjectReference Include="..\Mvc567.DataAccess\Mvc567.DataAccess.csproj" />
    <ProjectReference Include="..\Mvc567.Entities\Mvc567.Entities.csproj" />
  </ItemGroup>

</Project>
